<div class="profile-wrapper">
  <% 9.times do %>
    <div class="profile-card" data-controller="profile-card" data-action="click->profile-card#click">
      <div class="scene scene--card">
        <div class="card" data-profile-card-target="card">
          <div class="card__face card__face--front">
            <div class="profile-card">
              <img src="https://images.unsplash.com/photo-1509114397022-ed747cca3f65?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1035&q=80" alt="">
            </div>
          </div>
          <div class="card__face card__face--back">
              <iframe style="border-radius:12px;" src="https://open.spotify.com/embed/playlist/1UCY5fAnCk6w590OuAWvFS?utm_source=generator&theme=0" width="100%" height="100%" frameBorder="0" allowfullscreen="" allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture" loading="lazy"></iframe>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
